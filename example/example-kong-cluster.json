{
  "apiVersion": "enterprises.upmc.com/v1",
  "kind": "KongCluster",
  "metadata": {
    "name": "example-kong-cluster"
  },
  "spec": {
    "name": "example-kong-cluster",
    "replicas": 1,
    "useSamplePostgres": true,
    "apis": [{
      "name": "go-microservice",
      "upstream_url": "http://go-microservice.apps.svc.cluster.local:8080",
      "hosts": [
        "service-go.k8s.com"
      ]
    }],
    "plugins": [
      {
        "name": "jwt",
        "apis": [
          "go-microservice"
        ],
        "consumers": [{
          "username": "slokas"
        }]
      }
    ],
    "consumers": []
  }
}